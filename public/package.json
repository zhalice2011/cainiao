{
    "name": "es-ui",
    "version": "0.0.4",
    "description": "Smart Data Warehouse",
    "private": true,
    "engines": {
        "node": ">=8.0",
        "npm": ">=3.3"
    },
    "keywords": [
        "data-analysis",
        "system-monitoring"
    ],
    "author": "",
    "license": "ISC",
    "scripts": {
        "build:dll": "NODE_ENV=production node ./build/dll.js",
        "build:dev-dll": "node ./build/dll.js",
        "clean-prod": "rimraf dist/*",
        "clean-dev": "rimraf dist-dev/*",
        "clean-dll": "rimraf dll/* && rimraf dll-dev/*",
        "copy": "node ./build/copy.js",
        "dev": "npm run clean-dev && npm run copy --dev && npm run cutIcons && webpack-dev-server --config ./build/webpack.config.dev.js",
        "start": "NODE_ENV=development nodemon --watch .env --exec npm run dev",
        "prod": "npm run clean-prod && npm run copy && npm run cutIcons && webpack --config ./build/webpack.config.prod.js",
        "build": "NODE_ENV=production npm run prod",
        "build:combined": "NODE_ENV=production PUBLIC_PATH=/universe/ui npm run prod",
        "compile": "node tools/eslintCompile.js",
        "cutIcons": "node tools/cutIcons.js",
        "font": "node tools/fontDownload.js && node tools/fontReplace.js"
    },
    "dependencies": {
        "@guandata/guandata-design": "2.1.35",
        "@guandata/guandata-web-utils": "0.0.3",
        "@guandata/hooks": "^0.0.4",
        "@guandata/mock": "^0.1.0",
        "antd": "3.24.3",
        "axios": "^0.19.0",
        "classnames": "2.2.6",
        "core-js": "3",
        "d3": "3.5.17",
        "dagre": "^0.8.5",
        "event-emitter": "^0.3.5",
        "file-saver": "1.3.3",
        "gd-pan-zoom": "^0.0.1",
        "hoist-non-react-statics": "^3.3.0",
        "immer": "^3.2.0",
        "invariant": "^2.2.4",
        "isomorphic-style-loader": "5.1.0",
        "js-cookie": "^2.2.1",
        "keycode": "^2.2.0",
        "lodash": "^4.17.15",
        "moment": "2.24.0",
        "prop-types": "^15.7.2",
        "qs": "6.8.0",
        "ramda": "0.26.1",
        "react": "16.9.0",
        "react-dnd": "^9.4.0",
        "react-dnd-html5-backend": "^9.4.0",
        "react-dom": "16.9.0",
        "react-event-listener": "^0.6.6",
        "react-intl-universal": "^2.1.4",
        "react-markdown": "^4.3.1",
        "react-router-dom": "^5.1.2",
        "request": "^2.88.0",
        "ua-parser-js": "^0.7.20",
        "use-debounce": "^3.0.1",
        "use-immer": "0.3.3",
        "vega": "^5.6.0",
        "vega-tooltip": "^0.19.1"
    },
    "resolutions": {
        "react": "16.9.0"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-decorators": "^7.6.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.5.5",
        "@babel/preset-env": "^7.6.4",
        "@babel/preset-react": "^7.6.4",
        "@babel/preset-typescript": "^7.6.4",
        "@babel/runtime": "^7.5.5",
        "@types/jest": "^24.0.15",
        "@types/lodash": "^4.14.149",
        "@types/ramda": "^0.26.41",
        "@types/react": "16.9.0",
        "@types/react-dom": "16.9.0",
        "@typescript-eslint/eslint-plugin": "^2.20.0",
        "@typescript-eslint/parser": "^2.21.0",
        "assets-webpack-plugin": "^3.9.10",
        "autoprefixer": "^9.6.1",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "css-loader": "^3.2.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.3.0",
        "eslint-config-airbnb": "14.1.0",
        "eslint-config-airbnb-base": "11.1.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "4.0.0",
        "eslint-plugin-react": "6.10.0",
        "eslint-plugin-react-hooks": "^2.0.1",
        "file-loader": "^4.2.0",
        "fs-extra": "^8.1.0",
        "gaze": "^1.1.3",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "husky": "1.3.1",
        "jest": "^24.9.0",
        "node-http-proxy-json": "^0.1.9",
        "nodemon": "^2.0.4",
        "postcss-loader": "^3.0.0",
        "postcss-preset-env": "^6.7.0",
        "precss": "^4.0.0",
        "rimraf": "3.0.0",
        "shelljs": "0.8.3",
        "terser-webpack-plugin": "^2.1.0",
        "ts-jest": "^24.0.2",
        "typescript": "^3.7.2",
        "webpack": "4.39.3",
        "webpack-bundle-analyzer": "^3.6.0",
        "webpack-cli": "^3.3.7",
        "webpack-dev-server": "3.8.0",
        "webpack-merge": "4.2.2"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn jest"
        }
    }
}
